# Database connection (PostgreSQL from Railway)
DATABASE_URL=postgresql://user:password@host:5432/database

# Webshare API Key (get from https://www.webshare.io/)
WEBSHARE_API_KEY=your_api_key_here

# Cron schedule for scraping (default: daily at 6:00 AM)
SCRAPE_CRON="0 6 * * *"

# Set to true to run scraping immediately on start
RUN_ON_START=false
